{
  "name": "entity-extraction-poc",
  "version": "1.0.0",
  "description": "Universal Knowledge System with temporal intelligence and interactive topic exploration",
  "main": "src/pipeline/end-to-end.js",
  "type": "module",
  "scripts": {
    "start": "node src/pipeline/end-to-end.js",
    "test": "npm run test:api && npm run test:accuracy && npm run test:performance",
    "test:api": "node tests/run-tests.js",
    "test:quick": "node tests/quick-test.js",
