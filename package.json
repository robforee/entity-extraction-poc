    "api:demo": "node src/api/client-example.js",
    "phase3:demo": "node scripts/phase3-demo.js",
    "phase3:test": "node scripts/test-phase3-components.js",
    "lint": "eslint src/ --ext .js",
    "format": "prettier --write src/ evaluation/ config/",
    "clean": "rm -rf data/output/* logs/*",
    "backup": "node scripts/backup-data.js"
